<view class="uni-container"><uni-swiper-dot vue-id="6a0e5eef-1" info="{{info}}" current="{{current}}" mode="dot" dots-styles="{{dotsStyles}}" field="content" bind:__l="__l" vue-slots="{{['default']}}"><swiper data-event-opts="{{[['change',[['change',['$event']]]]]}}" class="swiper-box" bindchange="__e"><block wx:for="{{info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="{{['swiper-item',item.colorClass]}}"><image class="image" src="{{item.url}}" mode="aspectFill"></image></view></swiper-item></block></swiper></uni-swiper-dot><text class="uni-title">商铺列表</text><view class="uni-list"><block wx:for="{{feedList}}" wx:for-item="value" wx:for-index="index" wx:key="index"><block><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['goDetail',['$0'],[[['feedList','',index]]]]]]]}}" bindtap="__e"><view class="uni-media-list"><image class="uni-media-list-logo" src="{{value.cover_image}}"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{value.name}}</view><view class="uni-media-list-text-bottom">{{value.brief_info}}</view><view class="uni-media-list-text-pintuan">{{"起送 $"+value.pintuan+" | 配送 $0"}}</view></view></view></view></block></block></view></view>